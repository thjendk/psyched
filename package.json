{
  "name": "psyched",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "@apollo/client": "^3.0.0-beta.19",
    "@apollo/react-hooks": "^3.1.3",
    "@reduxjs/toolkit": "^1.3.1",
    "apollo-boost": "^0.4.7",
    "apollo-link-retry": "^2.2.15",
    "apollo-server-express": "^2.11.0",
    "bcrypt": "^4.0.1",
    "cookie-parser": "^1.4.5",
    "dataloader": "^2.0.0",
    "dotenv-flow": "^3.1.0",
    "eslint-plugin-graphql": "^3.1.1",
    "express": "^4.17.1",
    "formik": "^2.1.4",
    "graphql": "^14.6.0",
    "helmet": "^3.22.0",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.20.13",
    "lodash": "^4.17.15",
    "mysql": "^2.18.1",
    "node-sass": "^4.13.1",
    "objection": "^2.1.3",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-highlighter": "^0.4.3",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.4.1",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-react": "^0.88.2",
    "styled-components": "^5.0.1",
    "ts-node": "^8.8.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.8.3"
  },
  "scripts": {
    "start": "ts-node -P server/tsconfig.json -r tsconfig-paths/register build/server/server.js",
    "dev": "npm-run-all --parallel server client",
    "client": "react-scripts start",
    "server": "nodemon",
    "build": "npm-run-all --sequential build:client build:server",
    "test": "start-server-and-test testdev http://localhost:3000 cy:run",
    "test-coverage": "NODE_ENV=test jest --coverage",
    "eject": "react-scripts eject",
    "build:client": "react-scripts build",
    "build:server": "tsc -p server/tsconfig.json",
    "tsc": "tsc",
    "postbuild": "cd server && npx knex migrate:latest",
    "generate": "graphql-codegen --config codegen.yml",
    "cy": "cypress open",
    "cy:run": "cypress run",
    "db:reset": "cd server && NODE_ENV=\"test\" knex migrate:latest && NODE_ENV=\"test\" knex seed:run",
    "testdev": "cross-env NODE_ENV=\"test\" npm run dev"
  },
  "proxy": "http://localhost:3001",
  "eslintConfig": {
    "extends": "react-app",
    "plugins": [
      "graphql"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@bahmutov/add-typescript-to-cypress": "^2.1.2",
    "@graphql-codegen/cli": "^1.13.1",
    "@graphql-codegen/fragment-matcher": "1.13.1",
    "@graphql-codegen/introspection": "1.13.1",
    "@graphql-codegen/typescript": "1.13.1",
    "@graphql-codegen/typescript-operations": "^1.13.1",
    "@graphql-codegen/typescript-react-apollo": "^1.13.1",
    "@graphql-codegen/typescript-resolvers": "^1.13.1",
    "@testing-library/jest-dom": "^5.3.0",
    "@testing-library/react": "^10.0.1",
    "@testing-library/user-event": "^10.0.0",
    "@types/bcrypt": "^3.0.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/dotenv-flow": "^3.0.0",
    "@types/helmet": "0.0.45",
    "@types/jest": "^25.1.4",
    "@types/jsonwebtoken": "^8.3.8",
    "@types/lodash": "^4.14.149",
    "@types/node": "^13.9.0",
    "@types/react": "^16.9.26",
    "@types/react-dom": "^16.9.5",
    "@types/react-redux": "^7.1.7",
    "@types/react-router-dom": "^5.1.3",
    "@types/styled-components": "^5.0.1",
    "cypress": "^4.3.0",
    "npm-run-all": "^4.1.5",
    "start-server-and-test": "^1.10.11"
  }
}
